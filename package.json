{
  "name": "hubot-alias",
  "description": "Action alias for hubot",
  "version": "0.1.1",
  "author": "dtaniwaki",
  "license": "MIT",
  "keywords": [
    "hubot",
    "hubot-scripts"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dtaniwaki/hubot-alias.git"
  },
  "bugs": {
    "url": "https://github.com/dtaniwaki/hubot-alias/issues"
  },
  "dependencies": {
    "hubot": ">=2.3.0",
    "hubot-scripts": "*"
  },
  "devDependencies": {
    "blanket": "~1.1.6",
    "chai": "*",
    "coffee-script": "~1.6.3",
    "coveralls": "~2.5.0",
    "hubot-mock-adapter": "^1.0.0",
    "mocha": "*",
    "mocha-lcov-reporter": "0.0.1",
    "sinon": "*",
    "sinon-chai": "*"
  },
  "main": "./src/index.coffee",
  "scripts": {
    "test": "make test"
  },
  "config": {
    "blanket": {
      "pattern": "hubot-alias/src/scripts",
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules"
    }
  }
}
